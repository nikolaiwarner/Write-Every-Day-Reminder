{
  "manifest_version": 2,
  "name": "Write Every Day Reminder",
  "version": "1.0.1",
  "description": "A Chrome extension to remind you to write every day on 750words.com.",
  "background": {
    "scripts": ["jquery-1.7.2.min.js", "moment.min.js", "write_every_day_reminder.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "notifications",
    "http://750words.com/"
  ],
  "icons": { 
    "16":   "icon_success.png",
    "48":   "icon_success.png",
    "128":  "icon_128.png"
  }
}
